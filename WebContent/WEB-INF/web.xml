<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <filter>
  	<description>过滤用户非法请求</description>
  	<filter-name>UserFilter</filter-name>
  	<filter-class>com.corp.mms.user.servlet.UserFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>UserFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<description>用户登录验证</description>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.corp.mms.user.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>生成验证码</description>
    <servlet-name>CheckCodeServlet</servlet-name>
    <servlet-class>com.corp.mms.checkno.servlet.CheckCodeServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>获取职工信息</description>
    <servlet-name>WorkerServlet</servlet-name>
    <servlet-class>com.corp.mms.worker.servlet.WorkerServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>添加/修改职工信息</description>
    <servlet-name>AUWorkerServlet</servlet-name>
    <servlet-class>com.corp.mms.worker.servlet.AUWorkerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>查询会议室信息</description>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>com.corp.mms.home.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>添加/修改会议室信息</description>
    <servlet-name>AUHomeServlet</servlet-name>
    <servlet-class>com.corp.mms.home.servlet.AUHomeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>查询设备信息</description>
    <servlet-name>EquipServlet</servlet-name>
    <servlet-class>com.corp.mms.equip.servlet.EquipServlet</servlet-class>
  </servlet>
  <servlet>
    <description>添加/修改设备信息</description>
    <servlet-name>AUEquipServlet</servlet-name>
    <servlet-class>com.corp.mms.equip.servlet.AUEquipServlet</servlet-class>
  </servlet>
  <servlet>
    <description>查询会议室</description>
    <servlet-name>MeetingServlet</servlet-name>
    <servlet-class>com.corp.mms.meeting.servlet.MeetingServlet</servlet-class>
  </servlet>
  <servlet>
    <description>预约查询</description>
    <servlet-name>ApplicationQueryServlet</servlet-name>
    <servlet-class>com.corp.mms.meeting.servlet.ApplicationQueryServlet</servlet-class>
  </servlet>
  <servlet>
    <description>会议查询</description>
    <servlet-name>MeetingQueryServlet</servlet-name>
    <servlet-class>com.corp.mms.meeting.servlet.MeetingQueryServlet</servlet-class>
  </servlet>
  <servlet>
    <description>预约审核</description>
    <servlet-name>MeetingCheckServlet</servlet-name>
    <servlet-class>com.corp.mms.meeting.servlet.MeetingCheckServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/servlet/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckCodeServlet</servlet-name>
    <url-pattern>/CheckCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WorkerServlet</servlet-name>
    <url-pattern>/servlet/WorkerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AUWorkerServlet</servlet-name>
    <url-pattern>/servlet/AUWorkerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/servlet/HomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AUHomeServlet</servlet-name>
    <url-pattern>/servlet/AUHomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EquipServlet</servlet-name>
    <url-pattern>/servlet/EquipServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AUEquipServlet</servlet-name>
    <url-pattern>/servlet/AUEquipServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MeetingServlet</servlet-name>
    <url-pattern>/servlet/MeetingServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApplicationQueryServlet</servlet-name>
    <url-pattern>/servlet/ApplicationQueryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MeetingQueryServlet</servlet-name>
    <url-pattern>/servlet/MeetingQueryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MeetingCheckServlet</servlet-name>
    <url-pattern>/servlet/MeetingCheckServlet</url-pattern>
  </servlet-mapping>
	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
